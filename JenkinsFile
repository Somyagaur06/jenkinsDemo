pipeline{
    
    agent any
    environment{
    $PATH="/var/lib/jenkins/workspace/declarativeDemo/jenkinsDemo"
    PATH="/usr/share/maven/bin:$PATH"


    }
    
    stages{
        
       stage("Git checkout")
       {
           
           steps{
               
               git 'https://github.com/Somyagaur06/jenkinsDemo.git'
               
          }
          }
          
          
          stage("Maven Build")
          {
          
          steps{
  
  
         sh "mvn clean package"
         
          }
          
          
          }
          
          
        }
    
    
}
