pipeline{
    
    agent any
    environment{
    
    PATH="{$PATH}/jenkinsDemo"


    }
    
    stages{
        
       stage("Git checkout")
       {
           
           steps{
               
               git 'https://github.com/Somyagaur06/jenkinsDemo.git'
               
          }
          }
          
          
          stage("Maven Build")
          {
          
          steps{
  
         sh "mvn clean package"
          }
          
          
          }
          
          
        }
    
    
}
